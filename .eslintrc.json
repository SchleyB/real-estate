{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "installedESLint": true,
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": [2, {"skipUndeclared": true}],
        "no-console": 0,
        "max-len": [2, {"code": 120}],
        "react/jsx-no-bind": 0,
        "indent": ["error", 2],
        "jsx-a11y/no-static-element-interactions": 0,
        "react/jsx-filename-extension": 0,
        "comma-dangle": 1,
        "react/prefer-stateless-function": 1,
        "arrow-parens": 1,
        "arrow-body-style": 1,
        "dot-notation": 1,
        "no-param-reassign": 1
    }
}
